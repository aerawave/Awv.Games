<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Joshua Kinzer</Authors>
    <Company>Aerawave</Company>
    <Description>Awv.Games.WoW.Data is used to import data from CSV files generated by the absolutely wonderful site https://wow.tools/.

Then enables exporting of that data (for instance, in the case of items)

https://wow.tools/ literally made this entire Awv.Games.WoW project possible.</Description>
    <RepositoryUrl>https://github.com/aerawave/Awv.Games</RepositoryUrl>
    <PackageProjectUrl>https://github.com/aerawave/Awv.Games</PackageProjectUrl>
    <PackageReleaseNotes>Had to downgrade ImageSharp, as libwarcraft had problems with the latest beta of it.</PackageReleaseNotes>
    <Version>1.0.4</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="15.0.0" />
    <PackageReference Include="libwarcraft" Version="3.0.0-alpha1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="SixLabors.ImageSharp" Version="1.0.0-beta0006" />
  </ItemGroup>

</Project>
